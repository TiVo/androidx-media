<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="@dimen/playback_state_width"
    android:height="@dimen/playback_state_height"
    android:viewportWidth="48"
    android:viewportHeight="48">

    <path
        android:name="pause_left"
        android:pathData="@string/pause_left"
        android:strokeWidth="8"
        android:strokeColor="@color/tpPlayedColor"
        android:strokeLineCap="round" />

    <group android:name="pause_left_rotate"
        android:pivotX="24"
        android:pivotY="24">
        <path
            android:name="pause_right"
            android:pathData="@string/pause_right_pause"
            android:strokeWidth="8"
            android:strokeColor="@color/tpPlayedColor"
            android:strokeLineCap="round"
            android:strokeLineJoin="round" />
    </group>

</vector>