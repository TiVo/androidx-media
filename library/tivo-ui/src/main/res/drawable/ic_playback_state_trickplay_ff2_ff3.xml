<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/ic_playback_state_trickplay_fwd">

    <target android:name="speed2">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="300"
                android:propertyName="translateX"
                android:valueFrom="0"
                android:valueTo="13" />
        </aapt:attr>
    </target>
    <target android:name="speed3">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:startOffset="0"
                android:duration="300"
                android:propertyName="translateX"
                android:valueFrom="0"
                android:valueTo="26" />
        </aapt:attr>
    </target>
</animated-vector>